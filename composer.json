{
    "name": "rikodev/laravel-influxdb2",
    "description": "A service made to provide, set up and use the library from influxdata/influxdb-client-php in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tray Labs",
            "email": "dev@traylabs.com.br"
        },
        {
            "name": "RikoDEV",
            "email": "kontakt@riko.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "RikoDEV\\InfluxDB\\": "src/"
        }
    },
    "require": {
        "influxdata/influxdb-client-php": "^3.7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RikoDEV\\InfluxDB\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "InfluxDB": "RikoDEV\\InfluxDB\\Facades\\InfluxDB"
            }
        }
    }
}
